<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
    <servlet-name>facturasServlet</servlet-name>
    <servlet-class>com.kalimeradev.mipymee.server.FacturasServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>facturasServlet</servlet-name>
    <url-pattern>/mipymee/facturas</url-pattern>
  </servlet-mapping>
    
  <servlet>
    <servlet-name>profileServlet</servlet-name>
    <servlet-class>com.kalimeradev.mipymee.server.ProfileServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>profileServlet</servlet-name>
    <url-pattern>/mipymee/profile</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

    <security-constraint>
        <web-resource-collection>
            <web-resource-name>all</web-resource-name>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>*</role-name>
        </auth-constraint>
    </security-constraint>
    
</web-app>